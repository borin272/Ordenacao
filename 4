#include <stdio.h>
#include <string.h>

int main() {
    char nomes[5][50]; 
    char nomeBusca[50]; 
    int encontrado = 0; 

    printf("Digite cinco nomes:\n");
    for (int i = 0; i < 5; i++) {
        printf("Nome %d: ", i + 1);
        scanf("%s", nomes[i]);
    }

    printf("Digite o nome a ser buscado: ");
    scanf("%s", nomeBusca);

    for (int i = 0; i < 5; i++) {
        if (strcmp(nomes[i], nomeBusca) == 0) {
            printf("O nome '%s' foi encontrado na posição %d do vetor.\n", nomeBusca, i);
            encontrado = 1;
            break; 
        }
    }

    if (!encontrado) {
        printf("O nome '%s' não foi encontrado no vetor.\n", nomeBusca);
    }

    return 0;
}
